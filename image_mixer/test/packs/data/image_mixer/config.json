{
  "image_mixers": [
    {
      "output_folder": "RP/textures/output/",
      "output_template": "f{index}_{layer1}_icon.png",
      "combination_mode": "cartesian",
      "layers": [
        {
          "path": "RP/textures/vanilla_recipe_background.png",
          "blend_mode": "normal",
          "scale": 2
        },
        {
          "path": "{shaped_result}",
          "offset": [25, 0],
          "blend_mode": "normal",
          "scale": {
            "width": 32,
            "height": 32
          }
        }
      ]
    }
  ]
}
