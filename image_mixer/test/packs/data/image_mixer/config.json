{
  "image_mixers": [
    {
      "output_folder": "RP/textures/",
      "output_template": "filename_{index}_{layer1}_{layer2}.png",
      "combination_mode": "cartesian",
      "layers": [
        {
          "path": "RP/textures/background.png",
          "offset": [0, 0],
          "blend_mode": "normal"
        },
        {
          "path": [
            "RP/textures/frames/blue_frame.png",
            "RP/textures/frames/red_frame.png",
            "RP/textures/frames/green_frame.png"
          ],
          "offset": [0, 0],
          "blend_mode": "normal"
        },
        {
          "path": [
            "RP/textures/icons/blue.png",
            "none",
            "RP/textures/icons/green.png"
          ],
          "scale": [1, 2],
          "offset": [0, 0],
          "blend_mode": "normal"
        }
      ]
    }
  ]
}
