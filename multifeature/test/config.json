{
  "$schema": "https://raw.githubusercontent.com/Bedrock-OSS/regolith-schemas/main/config/v1.4.json",
  "author": "test",
  "name": "test",
  "packs": {
    "behaviorPack": "./packs/BP",
    "resourcePack": "./packs/RP"
  },
  "regolith": {
    "dataPath": "./packs/data",
    "filterDefinitions": {
      "jsonify": {
        "url": "github.com/thePixelmancer/regolith-filters",
        "version": "1.0.3"
      },
      "multifeature": {
        "runWith": "python",
        "script": "../multifeature.py"
      }
    },
    "formatVersion": "1.4.0",
    "profiles": {
      "default": {
        "export": {
          "build": "standard",
          "readOnly": false,
          "target": "local"
        },
        "filters": [
          {
            "filter": "jsonify",
            "settings": {
              "folders": ["BP/feature_rules", "BP/features", "BP/multifeatures"]
            }
          },
          {
            "filter": "multifeature",
            "settings": {
              "subfolders": "@team/@proj/"
            }
          }
        ]
      }
    }
  }
}
